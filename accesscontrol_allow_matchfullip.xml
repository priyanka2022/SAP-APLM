<!--This policy can be used to selectively whitelist or blacklist an IP or group of IPs. Enter the Source IP address in SourceAddress property -->
<AccessControl async='true' continueOnError='false' enabled='true' xmlns='http://www.sap.com/apimgmt'>
  <IPRules noRuleMatchAction='DENY'>
    <MatchRule action='ALLOW'>
      <SourceAddress mask='32'>XXX.XXX.XXX.XXX</SourceAddress>
    </MatchRule>
  </IPRules>
</AccessControl>
