	<CacheKey>
		<KeyFragment ref="request.uri"></KeyFragment>
	</CacheKey>
	<ExpirySettings>
		<TimeoutInSec ref="">3600</TimeoutInSec>
	</ExpirySettings>
	<SkipCacheLookup>request.header.InvalidateCache="true"</SkipCacheLookup>
	<SkipCachePopulation></SkipCachePopulation>
</ResponseCache>
